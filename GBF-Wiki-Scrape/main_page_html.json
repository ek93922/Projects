new Date();\n\t\tvar startDate = new Date(parseInt(el.dataset.startTime, 10));\n\t\tvar endDate = new Date(parseInt(el.dataset.endTime, 10));\n\n\t\tvar startDiff = startDate.getTime() - now.getTime();\n\t\tvar endDiff = endDate.getTime() - now.getTime();\n\t\t\n\t\tvar text = el.dataset.textEnd;\n\t\tvar diff = startDiff;\n\t\tif (startDiff > 1000) {\n\t\t\tif (startDate < endDate)\n\t\t\t\ttext = el.dataset.textStart;\n\t\t} else if (endDiff > 1000) {\n\t\t\tdiff = endDiff;\n\t\t} else {\n\t\t\ttext = el.dataset.textAfter;\n\t\t}\n\t\t\n\t\tif (diff < 1000) {\n\t\t\tel.innerHTML = text;// + ' (start: ' + startDate.toUTCString() + ', end: ' + endDate.toUTCString() + ')';\n\t\t\treturn;\n\t\t}\n\t\t\n\t\tdiff = diff / 1000;\n\t\tvar days   = Math.floor(diff / 86400);\n\t\tvar hours   = Math.floor(diff % 86400 / 3600);\n\t\tvar minutes = Math.floor(diff % 3600 / 60);\n\t\tvar seconds = (diff % 3600 % 60).toFixed(0);\n\n\t\tif (days === 1) { hours += 24; days = 0; }\n\t\tif (hours > 1 || days > 0) seconds = 0;\n\t\tif (days > 0) minutes = 0;\n\t\t\n\t\tvar result = formatTime(days, hours, minutes, seconds);\n\t\t\n\t\tvar interval = 1000;\n\t\tif (days > 0) {\n\t\t\tinterval = 60*60*1000;\n\t\t} else if (hours > 1) {\n\t\t\tinterval = 60*1000;\n\t\t}\n\t\t\n\t\tel.innerHTML = text.replace('%s', result);// + ' (start: ' + startDate.toUTCString() + ', end: ' + endDate.toUTCString() + ', diff: ' + diff +', interval: ' + interval + ')';\n\n\t\twindow.setTimeout(function() {\n\t\t\tupdateCountdownText(el);\n\t\t}, interval);\n\t}\n\n\n\tconst nth = function(d) {\n\t\tif (d > 3 && d < 21) return 'th';\n\t\tswitch (d % 10) {\n\t\t\tcase 1:  return \"st\";\n\t\t\tcase 2:  return \"nd\";\n\t\t\tcase 3:  return \"rd\";\n\t\t\tdefault: return \"th\";\n\t\t}\n\t}\n\n\tvar updateTimeText = function(el, time) {\n\t\tvar months = [\"January\",\"February\",\"March\",\"April\",\"May\",\"June\",\"July\",\"August\",\"September\",\"October\",\"November\",\"December\"];\n\t\tvar date = new Date(time);\n\t\tvar output = '';\n\t\tif ((el.dataset.format == 'hm') || (el.dataset.format == 'H:i')) {\n\t\t\toutput = (\"000\"+date.getHours()).slice(-2) + \":\" + \n\t\t\t\t(\"000\"+date.getMinutes()).slice(-2);\n\t\t} else if (el.dataset.format == 'md') {\n\t\t\toutput = months[date.getMonth()] + \" \" + \n\t\t\t\tdate.getDate() + nth(date.getDate());\n\t\t} else if (el.dataset.format == 'mdy') {\n\t\t\toutput = months[date.getMonth()] + \" \" +\n\t\t\t\tdate.getDate() + nth(date.getDate()) + \", \" +\n\t\t\t\tdate.getFullYear();\n\t\t} else {\n\t\t\toutput = (\"000\"+date.getHours()).slice(-2) + \":\" +\n\t\t\t\t(\"000\"+date.getMinutes()).slice(-2) + \", \" +\n\t\t\t\tmonths[date.getMonth()] + \" \" +\n\t\t\t\tdate.getDate() + nth(date.getDate()) + \", \" +\n\t\t\t\tdate.getFullYear();\n\t\t}\n\t\tel.innerHTML = output;\n\t}\n\t\n\tvar ready = function() {\n\t\tvar elements = document.querySelectorAll('.localtime');\n\t\tArray.from(elements).forEach(function(el) {\n\t\t\tvar startTime = verifyTime(el.dataset.start);\n\t\t\tvar endTime = verifyTime(el.dataset.end);\n\t\t\tvar time = verifyTime(el.dataset.time);\n\t\t\tel.dataset.textStart = verifyText(el.dataset.textStart, 'Starts in %s.', true);\n\t\t\tel.dataset.textEnd = verifyText(el.dataset.textEnd, 'Ends in %s.', true);\n\t\t\tel.dataset.textAfter = verifyText(el.dataset.textAfter, 'Event has ended.', false);\n\t\t\tif ((startTime + endTime) > 0) {\n\t\t\t\tel.dataset.startTime = startTime;\n\t\t\t\tel.dataset.endTime = endTime;\n\t\t\t\tupdateCountdownText(el);\n\t\t\t} else if (time > 0){\n\t\t\t\tupdateTimeText(el, time);\n\t\t\t}\n\t\t});\n\t}\n\t\n\tif (document.readyState == 'loading') {\n\t\tdocument.addEventListener('DOMContentLoaded', ready);\n\t} else {\n\t\tready();\n\t}\n})();\n</script><br /><div class=\"tooltip\"><span class=\"localtime\" data-start=\"1675238400\" data-end=\"1676620740\" data-text-start=\"Half AP etc. starts in\" data-text-end=\"Half AP etc. ends in\" data-text-after=\"Half AP has ended.\">Half AP etc. ends in 08:00 UTC, February 1, 2023</span><span class=\"tooltiptext\">17:00 JST, February 1, 2023<br />to<br />16:59 JST, February 17, 2023</span></div><br /><div class=\"tooltip\"><span class=\"localtime\" data-start=\"1675238400\" data-end=\"1676620740\" data-text-start=\"Pendant Boosts start in\" data-text-end=\"Pendant Boosts end in\" data-text-after=\"Pendant Boosts have ended.\">Pendant Boosts end in 08:00 UTC, February 1, 2023</span><span class=\"tooltiptext\">17:00 JST, February 1, 2023<br />to<br />16:59 JST, February 17, 2023</span></div><script type=\"text/javascript\">\n\"use strict\";\n\nwindow.addEventListener(\"load\", function() {\n  function nextOrFirstSibling(el) {\n    var next = el.nextElementSibling;\n    if (next) { return next; }\n    return el.parentElement.firstElementChild;\n  }\n\n  function initAccordion(el) {\n    var isPaused = false;\n    var images = el.querySelectorAll(\"a\");\n    \n    // Initialise\n    var img = el.querySelector(\"a img\");\n    var width = img.getAttribute(\"width\") || img.width;\n    var height = img.getAttribute(\"height\") || img.height;\n    images.forEach(function(image) {\n      image.style.width = `${width}px`;\n      image.style.height = `${height}px`;\n    });\n    images[0].classList.add(\"active\");\n    \n    // Rotation\n    var rotateAccordion = function() {\n      if (isPaused) return;\n      var curr = el.querySelector(\".active\");\n      var next = nextOrFirstSibling(curr);\n      \n      curr.classList.remove(\"active\");\n      next.classList.add(\"active\");\n    }\n    window.setInterval(rotateAccordion, 2000);\n    \n    // Hover\n    var hoverAction = function(event) {\n      isPaused = true;\n      el.querySelector(\".active\").classList.remove(\"active\");\n      this.classList.add(\"active\");\n    }\n    var blurAction = function(event) {\n      isPaused = false;\n    }\n    images.forEach(function(image) {\n      image.addEventListener(\"mouseover\", hoverAction);\n      image.addEventListener(\"mouseout\", blurAction);\n    });\n  }\n  \n  var el = document.querySelectorAll('.accordion-swap-images');\n  el.forEach(initAccordion);\n});\n</script>\n<div class=\"accordion-swap-images horizontal\" style=\"width: 900px; height: 80px; margin: 0 auto;\"><a href=\"/Special_Collab_Fever_Event#Free_Daily_Premium_Draw\" title=\"Special Collab Fever Event#Free Daily Premium Draw\"><img alt=\"News collabfeavercampaign 2.png\" src=\"/images/thumb/8/8a/News_collabfeavercampaign_2.png/311px-News_collabfeavercampaign_2.png\" decoding=\"async\" width=\"311\" height=\"80\" srcset=\"/images/thumb/8/8a/News_collabfeavercampaign_2.png/466px-News_collabfeavercampaign_2.png 1.5x, /images/8/8a/News_collabfeavercampaign_2.png 2x\" data-file-width=\"582\" data-file-height=\"150\" /></a><a href=\"/Special_Collab_Fever_Event#Special_Collab_Fever_Daily_Bonus\" title=\"Special Collab Fever Event#Special Collab Fever Daily Bonus\"><img alt=\"News specialcollabo campaign 2.png\" src=\"/images/thumb/9/96/News_specialcollabo_campaign_2.png/312px-News_specialcollabo_campaign_2.png\" decoding=\"async\" width=\"312\" height=\"80\" srcset=\"/images/thumb/9/96/News_specialcollabo_campaign_2.png/468px-News_specialcollabo_campaign_2.png 1.5x, /images/9/96/News_specialcollabo_campaign_2.png 2x\" data-file-width=\"581\" data-file-height=\"149\" /></a><a href=\"/Special_Collab_Fever_Event#3_Times_More_RP_and_EXP_for_Rank_150_and_Below\" title=\"Special Collab Fever Event#3 Times More RP and EXP for Rank 150 and Below\"><img alt=\"News collabfeavercampaign 3.png\" src=\"/images/thumb/b/b8/News_collabfeavercampaign_3.png/311px-News_collabfeavercampaign_3.png\" decoding=\"async\" width=\"311\" height=\"80\" srcset=\"/images/thumb/b/b8/News_collabfeavercampaign_3.png/466px-News_collabfeavercampaign_3.png 1.5x, /images/b/b8/News_collabfeavercampaign_3.png 2x\" data-file-width=\"582\" data-file-height=\"150\" /></a><a href=\"/Special_Collab_Fever_Event#Half_AP